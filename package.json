{
  "name": "@bj.anderson/cli-tool",
  "version": "1.1.1",
  "description": "A command line program for generating JavaScript and TypeScript projects and boilerplate code.",
  "author": "BJ Anderson",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:bjanderson/cli-tool.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "cli-tool": "./dist/index.js"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint ./src --ext .js,.ts",
    "prepush": "npm run build",
    "push": "npm publish --access public",
    "push:major": "npm version major --no-git-tag-version && npm run push",
    "push:minor": "npm version minor --no-git-tag-version && npm run push",
    "push:patch": "npm version patch --no-git-tag-version && npm run push",
    "watch": "npm-watch",
    "test": "jest",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@bj.anderson/utils": "5.3.0",
    "fs-extra": "9.0.1",
    "moment": "2.29.1"
  },
  "devDependencies": {
    "@types/fs-extra": "9.0.6",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.21",
    "@typescript-eslint/eslint-plugin": "4.13.0",
    "@typescript-eslint/eslint-plugin-tslint": "4.13.0",
    "@typescript-eslint/parser": "4.13.0",
    "eslint": "7.18.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "7.1.0",
    "eslint-import-resolver-typescript": "2.3.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "31.0.7",
    "eslint-plugin-json": "2.1.2",
    "eslint-plugin-prettier": "3.3.1",
    "import": "0.0.6",
    "jest": "26.6.3",
    "npm-watch": "0.7.0",
    "prettier": "2.2.1",
    "ts-jest": "26.4.4",
    "tslint": "5.20.1",
    "typescript": "4.1.3"
  },
  "files": [
    "dist/",
    "package.json",
    "README.md",
    "LICENSE"
  ]
}
