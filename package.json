{
  "name": "@bjanderson/cli-tool",
  "version": "1.2.0",
  "description": "A command line program for generating JavaScript and TypeScript projects and boilerplate code.",
  "author": "BJ Anderson",
  "license": "MIT",
  "publishConfig": {
    "@bjanderson:registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:bjanderson/cli-tool.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "cli-tool": "./dist/index.js"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "ts"
    }
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "lint": "eslint ./src --ext .js,.ts",
    "prepublish": "npm run build",
    "push:major": "npm version major",
    "push:minor": "npm version minor",
    "push:patch": "npm version patch",
    "watch": "npm-watch",
    "test": "jest",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@bjanderson/utils": "1.0.1",
    "fs-extra": "10.0.0",
    "moment": "2.29.1"
  },
  "devDependencies": {
    "@types/fs-extra": "9.0.11",
    "@types/jest": "26.0.23",
    "@types/node": "15.12.5",
    "@typescript-eslint/eslint-plugin": "4.28.1",
    "@typescript-eslint/eslint-plugin-tslint": "4.28.1",
    "@typescript-eslint/parser": "4.28.1",
    "eslint": "7.29.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsdoc": "35.4.1",
    "eslint-plugin-json": "3.0.0",
    "eslint-plugin-prettier": "3.4.0",
    "import": "0.0.6",
    "jest": "26.6.3",
    "npm-watch": "0.10.0",
    "prettier": "2.3.2",
    "rimraf": "3.0.2",
    "ts-jest": "26.5.6",
    "tslint": "5.20.1",
    "typescript": "4.3.4"
  },
  "files": [
    "dist/",
    "package.json",
    "README.md",
    "LICENSE"
  ]
}
